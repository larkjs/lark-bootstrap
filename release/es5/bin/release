#!/usr/bin/env node
'use strict'

let cp    = require('child_process');
let path  = require('path');

let binPath = __dirname;

console.log("Start to build release");
cp.execSync(path.join(binPath, 'export') + ' release');
cp.execSync(path.join(binPath, 'export') + ' release es5');
console.log("Release done");

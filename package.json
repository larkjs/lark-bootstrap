{
    "name": "lark-bootstrap",
    "version": "2.0.0-alpha",
    "description": "bootstrap by PM2",
    "main": ".easy",
    "engines": {
        "node": ">=4.0.0"
    },
    "directories": {
        "example": "examples"
    },
    "scripts": {
        "test": "cd .easy && rm -rf node_modules && ln -s ../node_modules node_modules && NODE_ENV=testing ./node_modules/.bin/mocha --harmony --require should test/"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/larkjs/lark-bootstrap.git"
    },
    "keywords": [
        "bootstrap",
        "lark"
    ],
    "dependencies": {
        "debug": "^2.2.0",
        "extend": "^3.0.0",
        "pm2": "^0.15.10"
    },
    "author": "lark-team",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/larkjs/lark-bootstrap/issues"
    },
    "homepage": "https://github.com/larkjs/lark-bootstrap",
    "devDependencies": {
        "chalk": "^1.1.1",
        "easy-babel": "^1.0.0",
        "mocha": "^2.3.3",
        "should": "^7.1.1"
    },
    "easy": {
        "main": "index.js",
        "scripts": "NODE_ENV=testing ./node_modules/.bin/mocha --harmony --require should test/"
    }
}